{
	"version": 3,
	"routes": [
		{
			"src": "^/github$",
			"headers": {
				"Location": "https://github.com/mackenzie"
			},
			"status": 301
		},
		{
			"src": "^/linkedin$",
			"headers": {
				"Location": "https://linkedin.com/in/mackenzie"
			},
			"status": 301
		},
		{
			"handle": "filesystem"
		},
		{
			"src": "^/_astro/(.*)$",
			"headers": {
				"cache-control": "public, max-age=31536000, immutable"
			},
			"continue": true
		},
		{
			"src": "^/.*$",
			"dest": "/404.html",
			"status": 404
		}
	]
}